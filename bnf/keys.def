# ------ Bottom left
double 2B 63
  =>  mouse "MOUSE_PREV" ;

double 2B 64
  =>  mouse "MOUSE_NEXT" ;

double 2B 74
  =>  keyboard "KEY_HOME" ;

double 2B 75
  =>  keyboard "KEY_END" ;

double 2B 76
  =>  keyboard "KEY_PAGE_UP" ;

double 2C 38
  =>  keyboard "KEY_PAGE_DOWN" ;