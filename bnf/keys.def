# https://github.com/NicoHood/HID/blob/master/src/KeyboardLayouts/ImprovedKeylayouts.h
# https://github.com/NicoHood/HID/blob/master/src/KeyboardLayouts/ImprovedKeylayoutsSE.h

# Demo
double 2A 30 => 
  keyboard down "KEY_LEFT_SHIFT" ,
  keyboard "KEY_A" ,
  keyboard "KEY_B" ,
  keyboard up "KEY_LEFT_SHIFT" ,
  keyboard "KEY_C" ;


# ------ QWERTY-part

# Escape
double 2C 68
  =>  keyboard "KEY_ESC" ;

# Backspace
single 7F
  =>  keyboard "KEY_BACKSPACE" ;

# Å
single 7D
  =>  keyboard "KEY_SE_ACIRCLE" ;

# Å
single 7C
  =>  keyboard "KEY_SE_OUMLAUT" ;

# Ä
single 7B
  =>  keyboard "KEY_SE_AUMLAUT" ;

# Enter
single D
  =>  keyboard "KEY_ENTER" ;

# GUI (under enter)
double 2C 67
  =>  keyboard "KEY_LEFT_GUI" ;

